@model Modelo.Cadastros.Instituicao
@{
    Layout = "_LayoutIES";
}

@await Html.PartialAsync("_StylePartial")

<div class="container">
    <nav class="navbar navbar-transparent bg-transparent">
        <div class="container-fluid">
            <h5>Exibindo uma instituição existente</h5>
        </div>
    </nav>
    <br />
    <div class="container">
        @{
            await Html.RenderPartialAsync("~/Areas/Cadastros/Views/Instituicao/_PartialDetailsContentCard.cshtml", Model);
        }
    </div>
    <div class="container text-center">
        <a asp-action="Edit" class="btn botaoLogin" asp-route-id="@Model.InstituicaoID" style="width:30%">Alterar</a>
        <a asp-action="Index" class="btn botaoLogin" style="width:30%">Voltar</a>    
    </div>
</div>
    